{
    "name": "@slimio/dependency-analyser",
    "version": "1.0.0",
    "description": "SlimIO - Dependency Analyser (Draw a network of all SlimIO Projects)",
    "main": "index.js",
    "scripts": {
        "start": "node index",
        "skip": "node index --skip"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/ProjectsView.git"
    },
    "keywords": [
        "SlimIO",
        "project",
        "package",
        "analyser"
    ],
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/ProjectsView/issues"
    },
    "homepage": "https://github.com/SlimIO/ProjectsView#readme",
    "dependencies": {
        "@polka/send-type": "^0.5.2",
        "dotenv": "^8.0.0",
        "httpie": "^1.1.2",
        "kleur": "^3.0.3",
        "make-promises-safe": "^5.0.0",
        "pacote": "^9.5.0",
        "polka": "^0.5.2",
        "repos": "^2.0.0",
        "serve-static": "^1.14.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.0.0",
        "@commitlint/config-conventional": "^7.6.0",
        "@slimio/eslint-config": "^2.0.4",
        "eslint": "^5.16.0",
        "husky": "^2.3.0"
    },
    "engines": {
        "node": ">=10"
    }
}
